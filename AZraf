from random import randrange
x = randrange(10)

z = ("Incorrect")

while z == ("Incorrect"):
    y = int(input ("please type a number: "))
    if x == y :
        print ("Correct")
        z = ("Correct")
    else: 
        print ("Incorrect")   
        if x>y:
            print ("Higher")
        else:
            print ("lower")
